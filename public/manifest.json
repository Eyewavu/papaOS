{
	"manifest_version": 2,
	"name": "Papaj Adware",
	"description": "",
	"version": "1.0.0",
	"background": {
		"scripts": ["background.js"]
	},
	
	"web_accessible_resources": [
	  "assets/*.jpg"
	],
	
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["contentScripts.js"]
	}],
	
	"permissions": [
		"tabs"
	]
}