{
  "compilerOptions": {
    "moduleResolution": "Node",
    "target": "es2017",  
    "module": "esnext",
    "rootDir": "./src",
    "outDir": "./public",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
  "include": ["./src"]
}